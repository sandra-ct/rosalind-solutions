with open(r"rosalind_rna.txt") as file:
    dna = str(file.read())

transcript = {'T':'U'}
valid = ['C','G','T','A']
rna = [""]

for i in dna: 
    if i in valid: 
        if 'T' == i:
            rna.append(transcript[i])
        else:
            rna.append(i)
        
seq = ''.join(rna)
    
print("DNA sequence:", dna, "\nRNA sequence:", seq)
