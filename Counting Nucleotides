s = "AGCTTTTCATTCTGACTGCAACGGGCAATATGTCTCTGTGTGGATTAAAAAAAGAGTGTCTGATAGCAGC"

adenine = 0
thymine = 0
cytosine = 0
guanine = 0

for n in s:
    if n == 'A':
        adenine += 1
    elif n == 'C':
        cytosine += 1
    elif n == 'G':
        guanine += 1
    elif n == 'T':
        thymine += 1
        
print("There are", adenine, "adenine bases,", cytosine, "cytosine bases,", guanine, "guanine bases, and", thymine, "thymine bases.")
